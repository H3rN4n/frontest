{
  "manifest_version": 2,

  "name": "Frontest",
  "description": "A Chrome extension to try your HTML, CSS, JavaScript code using Chrome DevTool.",
  "version": "1.0.1",

  "browser_action": {
    "default_icon": "assets/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
      "storage",
      "tabs", 
      "http://*/*", 
      "https://*/*"
  ]
}